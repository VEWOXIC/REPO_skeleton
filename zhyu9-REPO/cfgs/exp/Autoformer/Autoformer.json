{
    "exp":{
        "train":{
            "batchsize":8,
            "epochs":20,
            "lr":0.0001,
            "lr_adj":0,
            "shuffle": true,
            "drop_last": true
        },
        "val":{
            "batchsize":128,
            "shuffle": false,
            "drop_last": false
        },
        "test":{
            "batchsize":128,
            "shuffle": false,
            "drop_last": false
        },
