{
    "exp":{
        "train":{
            "training": true,
            "saved_model": true,
            "batchsize":8,
            "epochs":100,
            "lr":0.001,
            "lr_adj":1,
            "shuffle": true,
            "drop_last": true,
            "step_size": 100,
            "optimizer": "adam",
            "loss": "mae",
            "patience": 7,
            "verbose": true,
            "delta": 0
        },
        "valid":{
            "batchsize":8,
            "shuffle": true,
            "drop_last": true
        },
        "test":{
            "batchsize":8,
            "shuffle": false,
            "drop_last": true
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": true,
            "MAPE": true,
            "RSE": true,
            "CORR": true
        },
        "device":"cuda"
    },


    "model": { 
        "model_name":"STGCN",
        "num_nodes": 7,
        "feature_dim":2,
        "output_dim":1,
        "Ks": 3,
        "Kt": 3,
        "blocks": [[1, 32, 64], [64, 32, 128]],
        "dropout": 0,
        "graph_conv_type": "chebconv",
        "stgcn_train_mode": "quick",
        "bidir_adj_mx": true,
      
        "scaler": "standard",
        "load_external": false,
        "normal_external": false,
        "ext_scaler": "none",
        "add_time_in_day": true,
        "add_day_in_week": false,
      
        "max_epoch": 100,
        "learner": "rmsprop",
        "learning_rate": 0.001,
        "lr_decay": true,
        "lr_scheduler": "steplr",
        "lr_decay_ratio": 0.7,
        "step_size": 5,
        "clip_grad_norm": false,
        "use_early_stop": false,
        "UseTimeFeature": true
    },

    "data": {
        "dataset_name":"ETTh1",
        "path":"./datasets/ETTh1.csv",
        "freq": "h",
        "lookback":48,
        "horizon":24,
        "channel":7,
        "normalize": 3,
        "train_ratio":0.6,
        "valid_ratio":0.2,
        "test_ratio":0.2,
        "multivariate":true,
        "target": "OT",
        "scalar":"StandardScalar"
    }
}

