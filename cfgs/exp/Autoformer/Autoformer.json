{
    "exp":{
        "train":{
            "batchsize":8,
            "epochs":20,
            "lr":0.0001,
            "lr_adj":0,
            "shuffle": true,
            "drop_last": true
        },
        "valid":{
            "batchsize":128,
            "shuffle": false,
            "drop_last": false
        },
        "test":{
            "batchsize":128,
            "shuffle": false,
            "drop_last": false
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": false,
            "MAPE": false,
            "RSE": false,
            "CORR": false
        },
        "device":"cpu"
    },

    "model":{
        "model_name":"Autoformer",
        "pred_len" : 96,
        "seq_len"  : 96,
        "label_len": 48,
        "output_attention" : true,
        "moving_avg" : 25,
        "enc_in" : 7,
        "d_model": 512,
        "embed" : "timeF",
        "freq" :  "h",
        "dropout" : 0.05,
        "factor" : 1,
        "n_heads" : 8,
        "d_ff" : 2048,
        "activation" : "gelu",
        "e_layers" : 2,
        "c_out" : 7,
        "d_layers" : 1,
        "use_amp": false
       
    },
   

    "data":{
        "dataset_name":"ETTh1",
        "path":"./datasets/ETTh1.csv",
        "lookback":48,
        "horizon":24,
        "channel":7,
        "train_ratio":0.6,
        "valid_ratio":0.2,
        "test_ratio":0.2,
        "multivariate":true,
        "target":"OT",
        "scalar":"StandardScalar",
        "freq": "h"
     
    }
}
