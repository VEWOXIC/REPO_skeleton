{
    "exp":{
        "train":{
            "training": true,
            "saved_model": true,
            "batchsize":8,
            "epochs":100,
            "lr":0.001,
            "lr_adj":1,
            "shuffle": true,
            "drop_last": true,
            "optimizer": "adam",
            "loss": "mae",
            "patience": 7,
            "verbose": true,
            "delta": 0
        },
        "valid":{
            "batchsize":8,
            "shuffle": true,
            "drop_last": true
        },
        "test":{
            "batchsize":8,
            "shuffle": false,
            "drop_last": true
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": true,
            "MAPE": true,
            "RSE": true,
            "CORR": true
        },
        "device":"cuda"
    },

    "model":{
        "model_name":"gMLP",
        "num_layers":1,
        "batchsize":8,
        "c_in": 48,
        "c_out": 24,
        "seq_len": 255,
        "patch_size": 1,
        "d_model": 256,
        "d_ffn": 512,
        "depth": 6,
        "UseTimeFeature": false
    },

    "data":{   
        "exp_id": false,
        "dataset_name":"wind",
        "path":"./datasets/wind.csv",
        "lookback":48,
        "horizon":24,
        "channel":255,
        "normalize":0,
        "freq": "h",
        "train_ratio":0.6,
        "valid_ratio":0.2,
        "test_ratio":0.2,
        "multivariate":true,
        "target": "OT",
        "scalar":"StandardScalar"
    }
    
}
