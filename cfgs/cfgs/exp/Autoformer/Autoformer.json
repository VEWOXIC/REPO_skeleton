{
    "exp":{
        "train":{
            "batchsize":8,
            "epochs":100,
            "lr":0.001,
            "lr_adj":1,
            "shuffle": true,
            "drop_last": true
        },
        "valid":{
            "batchsize":8,
            "shuffle": true,
            "drop_last": true
        },
        "test":{
            "batchsize":8,
            "shuffle": false,
            "drop_last": true
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": true,
            "MAPE": true,
            "RSE": true,
            "CORR": true
        },
        "device":"cuda"
    },
    "model":{
        "model_name":"Autoformer",
        "bucket_size":4,
        "n_hashes":4,
        "enc_in":7,
        "dec_in":7,
        "c_out":7,
