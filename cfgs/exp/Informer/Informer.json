{
    "exp":{
        "train":{
            "batchsize":8,
            "epochs":100,
            "lr":0.001,
            "lr_adj":1,
            "shuffle": true,
            "drop_last": true
        },
        "valid":{
            "batchsize":8,
            "shuffle": true,
            "drop_last": true
        },
        "test":{
            "batchsize":8,
            "shuffle": false,
            "drop_last": true
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": true,
            "MAPE": true,
            "RSE": true,
            "CORR": true
        },
        "device":"cuda"
    },

    "model":{
        "model_name":"Informer",
        "output_attention":"False",
        "features":"M",
        "seq_len":96,
        "label_len":48,
        "pred_len":24,
        "d_model":512,
        "n_heads":8,
        "e_layers":2,
        "d_layers":1,
        "distil":"True",
        "d_ff":2048,
        "attn":"prob",
        "factor":5,
        "embed":"timeF",
        "enc_in":7,
        "freq":"h",
        "dropout":0.0,
        "activation":"gelu",
        "dec_in":7,
        "c_out":7,
        "mix":"True",
        "UseTimeFeature": "False",
        "use_amp": "False"
    },

    "data":{
        
            "dataset_name":"ETTh1",
            "path":"./datasets/ETTh1.csv",
            "lookback":48,
            "horizon":24,
            "channel":7,
            "train_ratio":0.6,
            "valid_ratio":0.2,
            "test_ratio":0.2,
            "multivariate":true,
            "target":"OT",
            "scalar":"StandardScalar",
            "freq":"h"
        }
}
