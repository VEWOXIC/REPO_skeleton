{
    "exp":{
        "train":{
            "batchsize":8,
            "epochs":10,
            "lr":0.001,
            "lr_adj":1,
            "shuffle": true,
            "drop_last": true
        },
        "valid":{
            "batchsize":8,
            "shuffle": true,
            "drop_last": true
        },
        "test":{
            "batchsize":8,
            "shuffle": false,
            "drop_last": true
        },
        "metrics":{
            "MSE": true,
            "MAE": true,
            "RMSE": true,
            "MAPE": true,
            "RSE": true,
